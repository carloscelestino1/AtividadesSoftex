import java.io.FileOutputStream;
import java.io.ObjectOutputStream;

public class NovoAluno{

    FileOutputStream = new FileOutputStream("c:\ alunos.xls");
    ObjectOutputStream = new ObjectOutputStream("alunos.xls");
    alunos.writeObject("alunos.xls");
    alunos.close();

}


